{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "es2016",
    "module": "es2015",
    "moduleResolution": "node",
    "jsx": "preserve",
    "lib": ["dom","es2015","es2016","es5","es6"],
    "typeRoots": ["node_modules/@types"]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "typings"
  ]
}